{
  "version": "1.2.0",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "browser-sync": "^2.27.10",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "glob": "^7.1.3",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-markdown": "^2.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "^3.0.1",
    "gulp-wrapper": "^1.0.0",
    "image-size": "^0.6.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "pixrem": "^4.0.1",
    "postcss-opacity": "^5.0.0",
    "protractor": "^5.4.1",
    "pump": "^1.0.3",
    "puppeteer": "^1.10.0",
    "rxjs": "^5.5.12",
    "sass-lint": "^1.12.1",
    "shelljs": "^0.8.3"
  },
  "scripts": {
    "build": "gulp",
    "e2e:prepare": "webdriver-manager update --gecko=false",
    "e2e:add-screen": "xrandr --output LVDS1 --mode 1366x768 --fb 2000x3000 --panning 2000x3000",
    "e2e:remove-screen": "xrandr -s 0",
    "e2e:run": "protractor e2e/protractor.conf.js",
    "e2e:ci": "npm-run-all e2e:ci:*",
    "e2e:ci:1": "npm-run-all -p e2e:ci:1:*",
    "e2e:ci:1:1": "npm run serve:background",
    "e2e:ci:1:2": "sleep 5; protractor e2e/protractor.conf.js",
    "e2e:ci:2": "cd e2e-screenshots; ./build-view.js",
    "lint": "sass-lint -c .scss-lint.yml",
    "serve": "gulp dev",
    "serve:background": "timeout 60s gulp dev; echo 0"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
